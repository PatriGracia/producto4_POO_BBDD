<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="grupoTeamRocket.modelo.Articulo" table="Articulo">
        <id name="idArticulo" column="id_articulo"></id>
        <property name="descripcion" type="string" column="descripcion" />
        <property name="precio" type="float" column="precio"/>
        <property name="gastos_envio" type="float" column="gastos_envio"/>
        <property name="tiempo_preparacion" type="float" column="tiempo_preparacion"/>

    </class>
</hibernate-mapping>